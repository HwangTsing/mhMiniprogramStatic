/*基础公共样式*/
page {
  position: relative;
  font-family: Arial, "Microsoft YaHei", sans-serif;
  overflow-x: hidden;
  background-color: #f5f5f5;
}
.container {
  width: 100%;
  display: flex;
  display: -webkit-box;
  display: -webkit-flex;
  flex-direction: column;
  -webkit-box-orient: vertical;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  align-items: center;
  font-size: 26rpx;
  padding: 0;
  overflow-x: hidden;
  box-sizing: border-box;
  position: relative;
  background: #fff;
}
.boy_layer,
.girl_layer {
  width: 100%;
  background: #000000;
  opacity: 0.8;
  -webkit-border-radius: 2rpx;
  -moz-border-radius: 2rpx;
  border-radius: 2rpx;
  text-align: center;
  height: 100%;
  z-index: 100;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.boy_layer_hidden,
.girl_layer_hidden {
  width: 100%;
  background: #000000;
  opacity: 0.8;
  -webkit-border-radius: 2rpx;
  -moz-border-radius: 2rpx;
  border-radius: 2rpx;
  text-align: center;
  height: 100%;
  position: fixed;
  bottom: 0;
  z-index: 100;
  left: 0;
  right: 0;
  display: none;
}
.boy_layer_img,
.girl_layer_img {
  width: 100%;
  -webkit-border-radius: 2rpx;
  -moz-border-radius: 2rpx;
  border-radius: 2rpx;
  text-align: center;
  height: 100%;
  z-index: 100;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.boy_layer_img_hidden,
.girl_layer_img_hidden {
  width: 100%;
  -webkit-border-radius: 2rpx;
  -moz-border-radius: 2rpx;
  border-radius: 2rpx;
  text-align: center;
  height: 100%;
  position: fixed;
  bottom: 0;
  z-index: 100;
  left: 0;
  right: 0;
  display: none;
}
#toast {
  width: 100%;
  height: 100%;
  -webkit-border-radius: 2rpx;
  -moz-border-radius: 2rpx;
  border-radius: 2rpx;
  z-index: 100;
  background: #000000;
  opacity: 0.7;
  display: block;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.toast_img {
  width: 100%;
  height: 100%;
  -webkit-border-radius: 2rpx;
  -moz-border-radius: 2rpx;
  border-radius: 2rpx;
  z-index: 100;
  display: block;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.toast_view {
  width: 100%;
