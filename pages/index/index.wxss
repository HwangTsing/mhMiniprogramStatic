/*搜索*/
.search_view {
  width: 100%;
  height: 92rpx;
  position: fixed;
  top: 0;
  z-index: 99;
  left: 0;
  right: 0;
}
.search_view_show {
  width: 100%;
  height: 92rpx;
  position: fixed;
  top: 0;
  z-index: 99;
  left: 0;
  right: 0;
  background: #fff;
  opacity: 0.95;
}
.search_img {
  width: 540rpx;
  height: 56rpx;
  margin-left: 30rpx;
  display: inline-block;
}
.search_img .del {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 0;
  right: 100px;
  margin-top: 28rpx;
}
.search_img .del_hidden {
  width: 30rpx;
  height: 30rpx;
  display: none;
}
.search_img .search_images {
  width: 40rpx;
  height: 40rpx;
  vertical-align: bottom;
  display: inline-block;
  float: left;
  position: absolute;
  left: 44rpx;
  top: 27rpx;
  z-index: 99;
}
.search_input {
  height: 56rpx;
  -webkit-border-radius: 30rpx;
  -moz-border-radius: 30rpx;
  -mz-border-radius: 30rpx;
  border-radius: 30rpx;
  /*text-align: center;*/
  padding-left: 52rpx;
  color: #fff;
  font-size: 28rpx;
  margin-top: 16rpx;
  margin-bottom: 16rpx;
  background: #000000;
  opacity: 0.2;
}
.search_img .boy_img,
.search_img .girl_img {
  width: 120rpx ;
  height: 60rpx;
  line-height: 60rpx;
  display: inline-block;
  position: absolute;
  top: 16rpx;
  right: 0;
  margin-right: 30rpx;
}
.search_img .boy_img_hidden,
.search_img .girl_img_hidden {
  width: 120rpx ;
  height: 60rpx;
  line-height: 60rpx;
  display: inline-block;
  position: absolute;
  top: 16rpx;
  right: 0;
  margin-right: 30rpx;
  display: none;
}
.qx_view {
  width: 100rpx;
  height: 60rpx;
  line-height: 60rpx;
