/* pages/comment/comment.wxss */
.releaseTable_container {
    background: #FFFFFF;
    width: 100%
}
.release_table_nav_box,.release_table_nav{
    width: 100%;
    height: 64rpx;
    background: #FF4D6A; 
}
.release_table_nav{
    box-sizing: border-box;
    padding: 0 15rpx;
    position: fixed;
    display: flex;
    align-items: center;
    top: 0;
    left: 0;
    background: #FF4D6A; 
    z-index: 999;
}
.release_table_nav_itme{
    flex: 1;
    padding:0 15rpx;
    text-align: center;
    color: #FFFFFF;
    font-size: 28rpx;
}
