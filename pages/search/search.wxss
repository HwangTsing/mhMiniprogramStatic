/*上拉加载loading*/
.load_view {
  width: 100%;
  display: flex;
  display: -webkit-box;
  display: -webkit-flex;
  flex-direction: row;
  justify-content: center;
}
.load_img_bg {
  width: 36rpx;
  height: 36rpx;
  text-align: center;
  margin-top: 22rpx;
}
.load_image {
  margin-right: 20rpx;
  width: 36rpx;
  height: 36rpx;
  display: inline-block;
  /* 设置默认样式，开启3d硬件加速 */
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  /* 设置动画，animation:动画名称 动画播放时长单位秒或微秒 动画播放的速度曲线linear为匀速 动画播放次数infinite为循环播放; */
  -webkit-animation: play 1s linear infinite;
  -moz-animation: play 1s linear infinite;
  animation: play 1s linear infinite;
}
.comment_message {
  font-size: 28rpx;
  text-align: center;
  line-height: 80rpx;
  color: #999;
  margin-left: 20rpx;
}
.bg_search_mess {
  width: 100%;
  height: 100% ;
  background: #fff;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  padding-top: 50%;
}
.net_view,
.server_view {
  width: 100%;
  height: 100%;
  display: flex;
  display: -webkit-flex;
  display: -webkit-box;
  flex-direction: column;
  -webkit-box-orient: vertical;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  z-index: 99;
}
@keyframes play {
  to {
    transform: rotate(360deg);
  }
  from {
    transform: rotate(0deg);
  }
}
