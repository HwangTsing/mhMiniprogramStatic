  top: 24rpx;
  z-index: 99;
}
.search_img .input_input {
  width: 450rpx;
  height: 56rpx;
  padding-left: 52rpx;
  color: #333;
  font-size: 28rpx;
}
.search_img .del {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 0;
  right: 20%;
  margin-top: 28rpx;
  z-index: 99;
}
.search_img .bg_del {
  width: 70rpx;
  height: 46rpx;
  position: absolute;
  top: 0;
  right: 19%;
  margin-top: 28rpx;
  z-index: 99;
}
.search_img .bg_del_hidden {
  width: 70rpx;
  height: 46rpx;
  position: absolute;
  top: 0;
  right: 19%;
  margin-top: 28rpx;
  z-index: 99;
  display: none;
}
.search_img .del_hidden {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 0;
  right: 20%;
  margin-top: 28rpx;
  z-index: 99;
  display: none;
}
.qx_view {
  width: 100rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  top: 16rpx;
  right: 0;
  color: #333333;
  font-size: 32rpx;
}
.pop_view {
  width: 100%;
  margin-top: 88rpx;
}
.pop_view .pop_search {
  width: 100%;
  height: 105rpx;
  display: flex;
  flex-direction: row ;
  justify-content: space-between;
}
.pop_view .pop_search .search_title {
  font-size: 26rpx;
  color: #646464;
  display: inline-block;
  margin-top: 30rpx;
  margin-left: 30rpx;
}
.pop_view .pop_search .refresh_view {
  margin-top: 30rpx;
  margin-right: 30rpx;
}
.refresh_view_hidden {
  margin-top: 30rpx;
  margin-right: 30rpx;
  display: none;
}
.pop_search .refresh_view .refresh_img {
  width: 48rpx;
  height: 48rpx;
  display: inline-block;
  vertical-align: bottom;
}
.pop_search .refresh_view .refresh_txt {
  font-size: 26rpx;
  color: #333333;
  display: inline-block;
  line-height: 48rpx;
}
.pop_view .search_list {
  width: 100%;
  display: flex;
  flex-direction: row;
  text-align: center;
  flex-wrap: wrap;
}
.search_list .main_title {
  font-size: 28rpx;
  color: #333333;
  padding: 12rpx 36rpx;
  background: #F4F4F4;
  margin: 15rpx 30rpx;
}
