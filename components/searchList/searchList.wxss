/* components/searchList/searchList.wxss */
.list_data {
  width: 100%;
  height: 100%;
  margin-top: 31rpx;
}
.img_view {
  width: 180rpx;
  height: 231rpx;
  flex: 1;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  float: left;
}
.img_view image {
  width: 180rpx;
  height: 231rpx;

}
.book_data {
  flex: 1;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  height: 231rpx;
  margin-top:40rpx;
  margin-left:207rpx;
}
.book_data .book_name {
  width:400rpx;
  color: #333333;
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom:14rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;

}
.book_data .author {
  margin-bottom:16rpx;
  display: block;
}
.book_data .explain {
    display:inline-block ;
}
.book_data .author, .book_data .explain {
    color: #999;
    font-size: 26rpx;
}
.cate_view {
    width:450rpx;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2;
}
.vcover {
  background-image: url('data:img/jpg;base64,');
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-size: 100% auto;
}
.hcover {
  background-image: url('data:img/jpg;base64,');
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-size: 100% auto;
}