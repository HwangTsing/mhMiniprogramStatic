/* components/searchList/searchList.wxss */
.list_data {
  width: 100%;
  height: 100%;
  margin-top: 31rpx;
}
.img_view {
  width: 180rpx;
  height: 231rpx;
  flex: 1;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  float: left;
}
.img_view image {
  width: 180rpx;
  height: 231rpx;

}
.book_data {
  display: inline-block;
  flex: 1;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  height: 231rpx;
  margin-top:40rpx;
  margin-left:24rpx;
}
.book_data .book_name {
  color: #333333;
  font-size: 32rpx;
  display: block;
  font-weight: bold;
  margin-bottom:14rpx;
}
.book_data .author {
  margin-bottom:16rpx;
}
.book_data .explain {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.book_data .author, .book_data .explain {
  color: #999;
  font-size: 26rpx;
  display: block;
}